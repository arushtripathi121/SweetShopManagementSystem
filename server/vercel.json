{
    "version": 2,
    "builds": [
        {
            "src": "./server/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/(.*)",
            "dest": "server/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "server/server.js"
        }
    ]
}